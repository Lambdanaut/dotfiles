Config {  font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
	, bgColor = "black"
	, fgColor = "grey"
	, borderColor = "black"
	, position = Top
	, lowerOnStart = True
	, commands = [--  Run Weather "KILM" ["-t","<station>: <tempC>C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
                      --, Run Wireless "wlan0" ["-t","( <essid> <qualitybar> )","-L","25","-H","75","--low","#FF0000","--normal","#F9FF00","--high","#00FF00"] 100
                        Run Network "wlan0" ["-L","0","-H","32","--normal","green","--high","red"] 100
		      , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                      , Run Battery [] 100
		      , Run Date "%a %b %_d %Y %r" "date" 10
                      , Run StdinReader
		      , Run Com "netcfg current" [] "ssid" 50
                      , Run Com "sh ~/.bin/volume.sh" [] "vol" 100
                      , Run Com "cat ~/.bin/currentNews" [] "news" 500
		      ]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%cpu% | %battery% | %wlan0% * %ssid% | %vol% | <fc=#ee9a00>%news%</fc>}{ %StdinReader% | <fc=#ee9a00>%date%</fc> "
}
